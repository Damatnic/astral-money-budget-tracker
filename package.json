{
  "name": "astral-money",
  "version": "2.0.0",
  "description": "Enterprise-grade financial management application with comprehensive security, monitoring, and production-ready features",
  "main": "src/app/page.tsx",
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --verbose",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:security": "npm run security:audit && npm run test:ci",
    "test:all": "npm run type-check && npm run lint:check && npm run test:security && npm run test:e2e",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:backup": "tsx scripts/backup-database.ts",
    "monitoring:health": "tsx scripts/health-check.ts",
    "monitoring:metrics": "tsx scripts/collect-metrics.ts",
    "docker:build": "docker build -t astral-money .",
    "docker:run": "docker run -p 3000:3000 astral-money",
    "production:build": "npm run type-check && npm run security:audit && npm run test:ci && npm run build",
    "production:start": "NODE_ENV=production npm start",
    "production:migrate": "npm run db:migrate:prod",
    "deploy:staging": "vercel --target=staging",
    "deploy:prod": "npm run production:build && vercel --prod",
    "postinstall": "prisma generate",
    "prepare": "husky install || echo 'Husky not available'",
    "analyze": "cross-env ANALYZE=true npm run build",
    "clean": "rm -rf .next out dist coverage playwright-report test-results",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "keywords": [
    "fintech",
    "budget-tracker",
    "enterprise",
    "security",
    "financial-management",
    "nextjs",
    "typescript",
    "prisma",
    "tailwindcss",
    "authentication",
    "monitoring",
    "production-ready"
  ],
  "author": {
    "name": "Astral Money Team",
    "email": "security@astralmoney.com",
    "url": "https://astralmoney.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Damatnic/astral-money-budget-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/Damatnic/astral-money-budget-tracker/issues"
  },
  "homepage": "https://astral-budget.vercel.app",
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@next/bundle-analyzer": "^14.2.13",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@sentry/nextjs": "^8.40.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cookie": "^0.7.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.460.0",
    "next": "^14.2.32",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "recharts": "^2.13.3",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "uuid": "^10.0.0",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eslint-config-next": "14.2.32",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie": "^0.6.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.32",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.11.3",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "ts-jest": "29.2.5",
    "typescript": "^5.6.3"
  },
  "optionalDependencies": {
    "@next/swc-win32-x64-msvc": "^14.2.32"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:ci"
    }
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "packageManager": "npm@9.6.7",
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}